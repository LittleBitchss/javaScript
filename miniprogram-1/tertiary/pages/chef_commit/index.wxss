/* tertiary/pages/chef_commit/index.wxss */
.chef_commit{
  height: 100vh;
  background-color: #e5e5e5;
  font-family: 'Medium';
}
.chef_commit .protocol {
  position: relative;
  font-size: 25rpx;
  height: 100%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  padding: 30rpx 30rpx;
  box-sizing: border-box;
  line-height: 50rpx;
  overflow: auto;
  height: calc(100% - 60rpx);
}
.chef_commit .protocol .title {
  display: flex;
  justify-content: center;
  align-items: center;
}

.chef_commit .btn{
  position: absolute;
  left: 0;
  bottom: 0;
  display: block;
  width: 100%;
  height: 60rpx;
  background-color: #4768f3;
  margin-top: 30rpx;
  margin-bottom: 0;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 0;
  font-size: 28rpx;
  font-family: 'Medium';
}