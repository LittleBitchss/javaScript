<!--tertiary/pages/admin_orderDetails/index.wxml-->
<view class="admin_orderDetails">
  <view class="top">
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="0"><text class="{{active0}} txt">基本信息</text></view>
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="1"><text class="{{active1}} txt">家宴选址</text></view>
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="2" wx:if="{{chefIf.basic.chef_name}}"><text class="{{active2}} txt">厨师信息</text></view>
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="3"><text class="{{active3}} txt">食材来源</text></view>
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="4"><text class="{{active4}} txt">卫生条件</text></view>
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="5" wx:if="{{operation.length!=0}}"><text class="{{active5}} txt">乡宴操作</text></view>
    <view class="item {{chefIf.basic.chef_name?'items':''}}" bindtap="taggle" data-index="6" wx:if="{{ig}}"><text class="{{active6}} txt">检查表</text></view>
  </view>
  <view class="midden">
    <view class="applicationDetailsInfo" wx:if="{{show == 0}}">
      <view class="map">
        <map id="maps" bindtap="clickMap" class="maps" longitude="{{basic.m_longitude}}" latitude="{{basic.m_latitude}}" scale="16">
          <image class="cover-image" src="{{domain}}mark.png"></image>
        </map>
      </view>
      <cover-view class="cover-view"></cover-view>
      <view class="info">
        <view class="info_item">
          <view class="info_item_left">地点</view>
          <view class="info_item_right">{{basic.m_address}}</view>
        </view>
        <view class="info_item">
          <view class="info_item_left">宴会类型</view>
          <view class="info_item_right">{{types[basic.m_type-1].mt_name}}</view>
        </view>
        <view class="info_item">
          <view class="info_item_left">开宴日期</view>
          <view class="info_item_right">{{basic.m_start_date}}</view>
        </view>
        <view class="info_item">
          <view class="info_item_left">举办天数</view>
          <view class="info_item_right">{{basic.m_holding_days}}</view>天
        </view>
        <view class="info_item">
          <view class="info_item_left">宴会人数</view>
          <view class="info_item_right">{{basic.m_banquet_number}}</view>人
        </view>
        <view class="info_item">
          <view class="info_item_left">联系方式</view>
          <view class="info_item_right">{{basic.m_phone}}</view>
        </view>
      </view>
    </view>
    <view class="approvalSiteSelect" wx:if="{{show == 1}}">
      <view class="approval_orderAddre">
        <view class="top">
          <image class="img imgs" src="{{Family}}"></image>
          <view class="img_txt img_txts {{fontColor11}}">自家举办</view>
          <image class="img imgss" src="{{hall}}"></image>
          <view class="img_txt img_txtss {{fontColor22}}">大礼堂</view>
        </view>
        <view class="cont">
          <view class="Family" wx:if="{{shows == 1}}">
            <view class="item">
              <view class="lefts">
                举办区域
              </view>
              <view class="typ">
                <text class="txt">{{basic.pname}}</text>
              </view>
            </view>
            <view class="item">
              <view class="lefts"></view>
              <view class="typ">
                <text class="txt">{{basic.cname}}</text>
              </view>
            </view>
            <view class="item">
              <view class="lefts"></view>
              <view class="typ">
                <text class="txt">{{basic.aname}}</text>
              </view>
            </view>
            <view class="item">
              <view class="lefts"></view>
              <view class="typ">
                <text class="txt">{{basic.sname}}</text>
              </view>
            </view>
            <view class="item">
              <view class="lefts"></view>
              <view class="typ">
                <text class="txt">{{basic.vname}}</text>
              </view>
            </view>
            <view class="item">
              <view class="lefts">详细地址</view>
              <view class="typ">
                <text class="txt">{{basic.m_address}}</text>
              </view>
            </view>
          </view>
          <view class="hallDetails" wx:if="{{shows == 2}}">
            <view class="details">
              <image class="cover" src="{{cover}}"></image>
              <view class="title">{{a_name}}</view>
              <view class="addr">
                <image class="addr_img" src="../../../icon/report_marks.png"></image>
                地址：{{address}}
              </view>
              <view class="tiem">
                <image class="tiem_img" src="../../../icon/report_clock.png"></image>
                营业时间：全天
              </view>
              <view class="hint">礼堂详情</view>
              <swiper class="hallSwiper" autoplay="true" interval="3500" circular="true" indicator-dots="true" previous-margin="30px" easing-function="easeInCubic" snap-to-edge="true" next-margin="40px">
                <swiper-item class="swiper-item" wx:for="{{swipers}}" wx:key="index">
                  <image class="item-img" src="{{item}}"></image>
                </swiper-item>
              </swiper>
              <view class="assure">
                <view class="assures">
                  <text class="txts">保障</text>
                  <view class="item">
                    <image class="item_img" src="../../../icon/report_tick.png"></image>
                    <text class="item_txt">食品安全责任险</text>
                  </view>
                </view>
              </view>
            </view>
            <view class="hallls">
              <view class="hallls_i">
                <view class="hallls_i_left">选择大厅</view>
                <view class="hallls_i_right">
                  <text class="i_name">{{a_name+hallxss}}</text>
                </view>
              </view>
              <view class="hallls_i">
                <view class="hallls_i_left">预约日期</view>
                <view class="hallls_i_right">
                  <view class="i_time">
                    <text class="i_time_txt">{{startDate}}</text>
                    至
                    <text class="i_time_txt i_time_txts">{{endDate}}</text>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
    <view class="approvalChefInfo" wx:if="{{show == 2}}">
      <view class="approvalChefInfo_top">
        <view class="photos">
          <view class="photos_Left">
            <view class="photos_Left_title">厨师照片</view>
            <view class="photos_Left_text">身穿厨师服头戴厨师帽个人图片</view>
          </view>
          <view class="photos_right">
            <view class="horn horn_top_left"></view>
            <view class="horn horn_top_right"></view>
            <view class="horn horn_bottom_left"></view>
            <view class="horn horn_bottom_right"></view>
            <image class="photo" src="{{chefIf.basic.chef_photo}}"></image>
          </view>
        </view>
        <view class="basic">
          <view class="item">
            <text class="left">姓名：</text>
            <view class="basicInfoInput">{{chefIf.basic.chef_name}}</view>
          </view>
          <view class="item">
            <text class="left">性别：</text>
            <view class="basicInfoInput">{{chefIf.basic.chef_sex}}</view>
          </view>
          <view class="item">
            <text class="left">年龄：</text>
            <view class="basicInfoInput">{{chefIf.basic.chef_age}}</view>
            <text class="right">岁</text>
          </view>
          <view class="item">
            <text class="left">厨龄：</text>
            <view class="basicInfoInput">{{chefIf.basic.chef_cooking_age}}</view>
            <text class="right">岁</text>
          </view>
          <view class="item">
            <text class="left">联系方式：</text>
            <view class="basicInfoInput">{{chefIf.basic.chef_phone}}</view>
          </view>
          <view class="item">
            <text class="left">擅长菜系：</text>
            <view class="basicInfoInput">{{chefIf.basic.cuisine}}</view>
          </view>
          <view class="item">
            <text class="left">工作地址：</text>
            <view class="basicInfoInput">{{chefIf.basic.region}}</view>
          </view>
          <view class="item Nobd">
            <view class="left"></view>
            <view class="basicInfoInput">{{chefIf.basic.chef_work_address}}</view>
          </view>
        </view>
        <view class="grade">
          <view class="grade_top">
            <text class="left">厨师等级：</text>
            <view class="basicInfoInput">{{grade[chefIf.basic.chef_level-1].name}}</view>
          </view>
          <view class="grade_bottom">
            <view class="photos">
              <view class="photos_Left">
                <view class="photos_Left_title">职业等级证书</view>
                <view class="photos_Left_text">正规渠道获取的职业等级证书</view>
              </view>
              <view class="photos_right">
                <view class="horn horn_top_left"></view>
                <view class="horn horn_top_right"></view>
                <view class="horn horn_bottom_left"></view>
                <view class="horn horn_bottom_right"></view>
                <image class="photo" src="{{chefIf.basic.chef_cheflicense}}"></image>
              </view>
            </view>
          </view>
        </view>
        <view class="IDCard">
          <view class="IDCard_top">
            <view class="identity_number">
              <text class="left">身份证号：</text>
              <view class="basicInfoInput">{{chefIf.basic.chef_idcard}}</view>
            </view>
            <view class="IDCard_date">
              <text class="left">身份证有效期至：</text>
              <view class="basicInfoInput">{{chefIf.basic.chef_validity_id_card}}</view>
            </view>
          </view>
          <view class="IDCard_bottom">
            <view class="photos">
              <view class="photos_Left">
                <view class="photos_Left_title">身份证正面</view>
                <view class="photos_Left_text">真实未过期的身份证人像面</view>
              </view>
              <view class="photos_right">
                <view class="horn horn_top_left"></view>
                <view class="horn horn_top_right"></view>
                <view class="horn horn_bottom_left"></view>
                <view class="horn horn_bottom_right"></view>
                <image class="photo" src="{{chefIf.basic.chef_idcard_path}}"></image>
              </view>
            </view>
            <view class="photos photoss">
              <view class="photos_Left">
                <view class="photos_Left_title">身份证反面</view>
                <view class="photos_Left_text">真实未过期的身份证国徽面</view>
              </view>
              <view class="photos_right">
                <view class="horn horn_top_left"></view>
                <view class="horn horn_top_right"></view>
                <view class="horn horn_bottom_left"></view>
                <view class="horn horn_bottom_right"></view>
                <image class="photo" src="{{chefIf.basic.chef_idcard_other_side_path}}"></image>
              </view>
            </view>
          </view>
        </view>
        <view class="license">
          <view class="license_number">
            <text class="left">营业执照编号：</text>
            <view class="basicInfoInput">{{chefIf.basic.chef_license}}</view>
          </view>
          <view class="license_bottom">
            <view class="photos">
              <view class="photos_Left">
                <view class="photos_Left_title">营业执照</view>
                <view class="photos_Left_text">正规渠道获取的营业执照照片</view>
              </view>
              <view class="photos_right">
                <view class="horn horn_top_left"></view>
                <view class="horn horn_top_right"></view>
                <view class="horn horn_bottom_left"></view>
                <view class="horn horn_bottom_right"></view>
                <image class="photo" src="{{chefIf.basic.chef_license_path}}"></image>
              </view>
            </view>
            <view class="photos photoss">
              <view class="photos_Left">
                <view class="photos_Left_title">健康证</view>
                <view class="photos_Left_text">正规渠道获取的健康证照片</view>
              </view>
              <view class="photos_right" bindtap="upload" data-item="6">
                <view class="horn horn_top_left"></view>
                <view class="horn horn_top_right"></view>
                <view class="horn horn_bottom_left"></view>
                <view class="horn horn_bottom_right"></view>
                <image class="photo" src="{{chefIf.basic.chef_health_certificate}}"></image>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
