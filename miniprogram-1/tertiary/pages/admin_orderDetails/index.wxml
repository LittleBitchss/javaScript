    <view class="applicationDetailsHealth" wx:if="{{show == 4}}">
      <view class="applicationDetailsHealth_top">
        <view class="item">
          <view class="project">使用水源<text class="colon">:</text></view>
          <checkbox-group class="group">
            <label class="option1 lab">
