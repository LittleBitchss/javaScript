/* secondary/pages/editResume/index.wxss */
.editResume{
  height: 100vh;
  overflow: auto;
}
.editResume .item{
  min-height: 190rpx;
  width: 696rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid #cfcfcf;
  padding: 40rpx 36rpx 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.editResume .info{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 190rpx;
  width: 696rpx;
  margin: 0 auto;
  border-bottom: 2rpx solid #cfcfcf;
  padding: 0 36rpx;
}
.editResume .info .infos .name{
  font-size: 45rpx;
  font-family: 'Bold';
  display: flex;
  align-items: center;
}
.editResume .info .infos .name .name_right{
  width: 29rpx;
  height: 31rpx;
  margin-left: 16rpx;
}
.editResume .info .infos .describe{
  font-family: 'Regular';
  font-size: 25rpx;
}
.editResume .info .photo{
  width: 114rpx;
  height: 114rpx;
  border-radius: 50%;
  overflow: hidden;
  background-image: url(https://qunyan.canancn.com/assets/applet/img/jobRecruitment/photo.png);
  background-size: 100% 100%;
  box-shadow: #0186fe 0rpx 0rpx 20rpx -8rpx;
}
.editResume .info .photo .photos{
  width: 100%;
  height: 100%;
}

.editResume .item .title{
  font-family: 'Medium';
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 1;
}
.editResume .item .title .img{
  width: 29rpx;
  height: 31rpx;
}
.editResume .item .content{
  font-family: 'Regular';
  font-size: 22rpx;
  color: #5f5f5f;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 20rpx 0;
}
.editResume .item .content .content_left{
  display: flex;
  flex-direction: column;
}
.editResume .item .content .content_right{
  display: flex;
  align-items: center;
}
.editResume .item .content .content_left .content_left_top{
  color: #000;
  font-family: 'Medium';
  font-size: 25rpx;
}
.editResume .item .content .content_left .content_left_tops{
  font-family: 'Regular';
}
.editResume .item .content .arrows{
  width: 16rpx;
  height: 28rpx;
  margin-left: 15rpx;
}


.page-container .z_info{
  height: 100%;
  padding-top: 80rpx;
  position: relative;
}
.page-container .z_info .z_item{
  height: 160rpx;
  display: flex;
  align-items: center;
  margin: 0 22rpx;
  border-bottom: 2rpx solid #cfcfcf;
  justify-content: space-between;
  padding: 0 35rpx;
}
.page-container .z_info .z_item .z_items{
  width: 100%;
}
.page-container .z_info .z_item .z_item_left{
  font-family: 'Regular';
  font-size: 20rpx;
  color: #5f5f5f;
}
.page-container .z_info .z_item .z_item_left .z_item_left_txt{
  font-family: 'Medium';
  font-size: 30rpx;
  color: #000;
}
.page-container .z_info .z_item .photo{
  width: 114rpx;
  height: 114rpx;
  border-radius: 50%;
  overflow: hidden;
  background-image: url(https://qunyan.canancn.com/assets/applet/img/jobRecruitment/photo.png);
  background-size: 100% 100%;
  box-shadow: #0186fe 0rpx 0rpx 20rpx -8rpx;
}
.page-container .z_info .z_item .photo .photos{
  width: 100%;
  height: 100%;
}
.page-container .z_info .z_item .z_title{
  width: 100%;
  font-family: 'Regular';
  font-size: 25rpx;
  color: #5f5f5f;
}
.page-container .z_info .z_item .z_title .placeholder{
  width: 100%;
  font-family: 'Regular';
  font-size: 25rpx;
  color: #cfcfcf;
}
.page-container .z_info .z_item .z_title .z_input{
  width: 100%;
  font-size: 30rpx;
  color: #000;
  margin: 20rpx 0 0;
}
.page-container .z_info .z_item .choose{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.page-container .z_info .z_item .choose .choose_txt{
  width: 100%;
  font-family: 'Regular';
  font-size: 25rpx;
  color: #cfcfcf;
}
.page-container .z_info .z_item .choose .choose_arrows{
  width: 16rpx;
  height: 26rpx;
}
.page-container .z_info .z_info_bottom{
  width: 100%;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  bottom: 0;
  left: 0;
}
.page-container .z_info .z_info_bottom .btn{
  margin: 0 30rpx;
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #4768f3;
  color: #fff;
  font-size: 30rpx;
  font-family: 'Medium';
  border-radius: 5rpx;
}
.page-container .z_advantage{
