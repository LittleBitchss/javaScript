  height: 44rpx;
  background-color: #b1c5f3;
  color: #4768f3;
  font-size: 20rpx;
  margin: 70rpx auto 12rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 16rpx;
}
.findChef .notices .bulletin .bulletin_left{
  display: flex;
  justify-content: center;
  align-items: center;
}
.findChef .notices .bulletin .bulletin_left .bulletin_left_img{
