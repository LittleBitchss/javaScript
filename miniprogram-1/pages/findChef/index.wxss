/* pages/findChef/index.wxss */
.findChef{
  height: 100vh;
  background-color: #e5e5e5;
  position: relative;
  font-family: 'Medium';
  display: flex;
  overflow: hidden;
  flex-direction: column;
}
.findChef .custom-bar__wrapper {
  width: 100%;
  left: 24rpx;
  padding: 0 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.findChef .search-group {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.findChef .search-group .select{
  height: 100%;
  display: flex; 
  align-items: center;
  justify-content: space-between;
  color: #e70013;
  font-size: 25rpx;
}
.findChef .search-group .select .down{
  width: 16rpx;
  height: 10rpx;
  margin-left: 20rpx;
  display: block;
}
.findChef .selects{
  z-index: 200;
  width: 718rpx;
  border-radius:30rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  background-color: #fff;
  font-family: 'Regular';
  box-shadow: 2rpx 2rpx 6rpx -2rpx #579cff;
}
.findChef .selects .select{
  min-height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.findChef .selects .select .childs{
  display: flex; 
  width: 200rpx;
  align-items: center;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  position: relative;
}
.findChef .selects .select .childs .txt{
  max-width: 150rpx;
  min-width: 100rpx;
  text-align: center;
  overflow: hidden; 
  white-space: nowrap; 
  text-overflow: ellipsis; 
}
.findChef .selects .select .childs .fontColor{
  color: #4768f3;
}
.findChef .selects .select .childs .down{
  width: 16rpx;
  height: 10rpx;
  margin-left: 10rpx;
  display: block;
}
@keyframes up{
  0%{
    transform: rotate(180deg);
  }
  100%{
    transform: rotate(0);
  }
}
@keyframes downs{
  0%{
    transform: rotate(-180deg);
  }
  100%{
    transform: rotate(0);
  }
}
.findChef .selects .select .childs .up{
  transform: rotate(0);
  animation:up 300ms;
}
.findChef .selects .select .childs .downs{
  transform: rotate(0);
  animation:downs 300ms;
}
.findChef .selects .distance{
  background-color: #fff;
  border-top: 2rpx solid #aaa;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
@keyframes stretch{
  0%{
    max-height: 0;
  }
  100%{
    max-height: 360rpx;
  }
}
@keyframes shrink{
  0%{
    max-height: 360rpx;
  }
  100%{
    max-height: 0;
  }
}
.findChef .selects .distance .stretch{
  max-height: 360rpx;
  animation:stretch 500ms;
}
.findChef .selects .distance .shrink{
  max-height: 0;
  animation:shrink 500ms;
}
.findChef .selects .distance .options{
  width: 84%;
  height: 60rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.findChef .selects .distance .options .distance_txt{
  font-size: 22rpx;
}
.findChef .selects .distance .options .fontColor{
  color: #4768f3;
}
.findChef .selects .distance .options .distance_img{
  width: 32rpx;
  height: 22rpx;
}
.findChef .selects .goodAt{
  background-color: #fff;
  border-top: 2rpx solid #aaa;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  padding: 32rpx 0 24rpx 50rpx;
}
.findChef .selects .goodAt .option{
  display: flex;
  flex-wrap: wrap;
}
.findChef .selects .goodAt .option .options{
  width: 100rpx;
  height: 34rpx;
  border: 2rpx solid #f2f2f2;
  background-color: #f2f2f2;
  border-radius: 5rpx;
  font-size: 22rpx;
  margin: 0 30rpx 20rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.findChef .selects .goodAt .option .active{
  color: #4768f3;
  border-color: #4768f3;
}
.findChef .selects .goodAt .btn{
  display: flex;
  justify-content: center;
  align-items: center;
}
.findChef .selects .goodAt .btn .btns{
  width: 110rpx;
  height: 38rpx;
  background-color: #4768f3;
  color: #fff;
  border-radius: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 25rpx;
  padding: 0;
  margin-top: 10rpx;
}
.findChef .selects .grade{
  background-color: #fff;
  border-top: 2rpx solid #aaa;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
}
.findChef .selects .grade .stretch{
  max-height: 360rpx;
  animation:stretch 500ms;
}
.findChef .selects .grade .shrink{
  max-height: 0;
  animation:shrink 500ms;
}
.findChef .selects .grade .options{
  width: 84%;
  height: 60rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.findChef .selects .grade .options .distance_txt{
  font-size: 22rpx;
}
.findChef .selects .grade .options .fontColor{
  color: #4768f3;
}
.findChef .selects .grade .options .distance_img{
  width: 32rpx;
  height: 22rpx;
}
