}
.findChef .chefs .chefs_info .medal .medal_img{
  width: 40rpx;
  height: 44rpx;
  display: block;
  margin-right: 10rpx;
}

.findChef .chefs .chefs_info .chefs_first{
  display: flex;
  font-size: 30rpx;
}
.findChef .chefs .chefs_info .chefs_first .chefs_first_name{
