.findChef .map{
  width: 100%;
  overflow: hidden;
  flex: 1;
}
.findChef .map .maps{
  width: 100%;
  height: 150%;
  margin-top: -60%;
}
.findChef .cover-image {
  height: 50rpx;
  width: 36rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -50rpx;
  margin-left: -18rpx;
}
.findChef .mapList{
  background-color: #eee;
}
.findChef .notices{
  position:absolute;
  box-sizing: border-box;
  left: 0;
  bottom:-90%;
  width: 100%;
  height: 100%;
  background:linear-gradient(to bottom,rgb(255,255,255,0) 0%,#f0f0f0 12%);
}
::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}
@keyframes upToDown{
  0%{
    bottom:-90%;
  }
  100%{
    bottom:0;
  }
}
@keyframes middleToDown{
  0%{
    bottom:-40%;
  }
  100%{
    bottom:0;
  }
}
@keyframes middleToUp{
  0%{
    bottom:-40%;
  }
  100%{
    bottom:-90%;
  }
}
@keyframes downToMiddle{
  0%{
    bottom:0;
  }
  100%{
    bottom:-40%;
  }
}
.findChef .upToDown{
  bottom:0;
  animation:upToDown 300ms;
}
.findChef .middleToDown{
  bottom:0;
  animation:middleToDown 300ms;
}
.findChef .middleToUp{
  bottom:-90%;
  animation:middleToUp 300ms;
}
.findChef .downToMiddle{
  bottom:-40%;
  animation:downToMiddle 300ms;
}
/* 需添加 */
