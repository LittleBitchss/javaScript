  font-size: 34rpx;
  line-height: 1;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_top .userCard .userCard_editResume {
  font-family: 'Regular';
  font-size: 22rpx;
  line-height: 1;
  color: #5f5f5f;
  margin-top: 12rpx;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_state {
  width: calc(100% - 36rpx);
  height: 34rpx;
  margin: 122rpx auto 18rpx;
  display: flex;
  align-items: center;

}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_state .mine_state_item {
  height: 100%;
  display: flex;
  line-height: 1;
  justify-content: center;
  font-family: 'Light';
  font-size: 25rpx;
  margin: 0 28rpx;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_state .active {
  font-family: 'Medium';
  border-bottom: 4rpx solid #f14c44;
  color: #f14c44;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .list {
  height: calc(100% - 326rpx - 34rpx - 140rpx);
}

.jobRecruitment .jobSearch .jobSearch_top .mine .list .list_item4 .list_item4_right {
  display: flex;
  align-items: center;
  line-height: 1;
  font-family: 'Medium';
  font-size: 25rpx;
  color: #000;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .list .list_item4 .list_item4_right .state {
  width: 14rpx;
  height: 14rpx;
  background-color: #00c709;
