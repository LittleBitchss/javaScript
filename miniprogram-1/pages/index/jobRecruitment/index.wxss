  background-color: #fff;
  border-radius: 15rpx;
  padding: 16rpx 20rpx 8rpx;
}

.list_item .list_item1 {
  font-size: 28rpx;
  font-family: 'Medium';
  color: #4768f3;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 42rpx;
}

.list_item .list_item2 {
  line-height: 1;
  color: #f47c77;
  font-size: 24rpx;
  font-family: 'Regular';
}

.list_item .list_item3 {
  height: 52rpx;
  display: flex;
  align-items: center;
}

.list_item .list_item3 .list_item3_item {
  padding: 6rpx 12rpx;
  background-color: #eee;
  border-radius: 8rpx;
  line-height: 1;
  font-size: 18rpx;
  margin-right: 18rpx;
  font-family: 'Medium';
}

.list_item .list_item4 {
  height: 42rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: 'Medium';
}

.list_item .list_item4 .list_item4_left {
  display: flex;
  align-items: center;
  font-size: 18rpx;
}

.list_item .list_item4 .list_item4_left .photo {
  height: 42rpx;
  width: 42rpx;
  background-color: #4768f3;
  border-radius: 50%;
  display: block;
  margin-right: 14rpx;
}

.list_item .list_item4 .list_item4_right {
  font-size: 22rpx;
  color: #b6b6b6;
}

.jobRecruitment .jobSearch .jobSearch_top .mine {
  height: 100%;
}

