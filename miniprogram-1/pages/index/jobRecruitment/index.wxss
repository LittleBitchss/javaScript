/* pages/index/jobRecruitment/index.wxss */
.jobRecruitment {
  height: 100vh;
  background-color: #f3f3f3;
}

.jobRecruitment .jobSearch {
  height: 100%;
}

.jobRecruitment .jobSearch .jobSearch_top {
  height: 92%;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare {
  height: 100%;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top {
  height: 70rpx;
  display: flex;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .scroll {
  width: calc(100% - 96rpx);
  margin-left: 18rpx;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_left {
  height: 100%;
  display: flex;
  align-items: center;
  font-size: 25rpx;
  font-family: 'Light';
  color: #a6a6a6;

}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_left .item {
  min-width: 130rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

@keyframes zoom {
  0% {
    font-size: 25rpx;
  }

  100% {
    font-size: 34rpx;
  }
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_left .item {
  min-width: 130rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_left .active {
  font-size: 34rpx;
  font-family: 'Medium';
  color: #4768f3;
  min-width: 174rpx;
  animation: zoom 0.3s;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_right {
  width: 96rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_right .cross {
  width: 20rpx;
  height: 4rpx;
  background-color: #4768f3;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_top .jobSearchSquare_top_right .vertical {
  width: 4rpx;
  height: 20rpx;
  background-color: #4768f3;
  position: absolute;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_filter {
  width: calc(100% - 36rpx);
  margin: 0 auto;
  height: 56rpx;
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  font-family: 'Normal';
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_filter_left {
  display: flex;
  align-items: center;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_filter_left .jobSearchSquare_filter_left_item {
  width: 86rpx;
  display: flex;
  justify-content: center;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_filter_left .active {
  color: #4768f3;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_filter_right {
  display: flex;
  align-items: center;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .jobSearchSquare_filter_right .jobSearchSquare_filter_right_item {
  height: 34rpx;
  background-color: #e0e5f9;
  border-radius: 5rpx;
  margin-left: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 8rpx 12rpx;
}

.jobRecruitment .jobSearch .jobSearch_top .jobSearchSquare .list {
  height: calc(100% - 70rpx - 56rpx);
}

.list .list_item {
  width: 722rpx;
  height: 186rpx;
