  margin-top: 5rpx;
}

.jobRecruitment .jobSearch .jobSearch_bottom {
  height: 8%;
  background-color: #fff;
  border-top: 2rpx solid;
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-top: 10rpx;
}

.jobRecruitment .jobSearch .jobSearch_bottom .item {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.jobRecruitment .jobSearch .jobSearch_bottom .item .img {
  width: 48rpx;
  height: 48rpx;
}

.jobRecruitment .jobSearch .jobSearch_bottom .item .text {
  font-family: 'Medium';
  font-size: 24rpx;
  color: #b6b6b6;
}

.jobRecruitment .jobSearch .jobSearch_bottom .item .fontColor {
  color: #4768f3;
}

