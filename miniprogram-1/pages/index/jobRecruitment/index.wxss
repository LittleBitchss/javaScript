  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 162rpx;
  left: 50%;
  transform: translateX(-50%);
  box-shadow: #0186fe 0rpx 0rpx 20rpx -8rpx;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_top .userCard .userCard_photos {
  width: 114rpx;
  height: 114rpx;
  background-color: #fff;
  border-radius: 50%;
