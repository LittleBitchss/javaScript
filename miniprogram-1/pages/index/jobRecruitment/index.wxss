  margin: 0 0 14rpx;
  overflow: hidden;
  background-image: url(https://qunyan.canancn.com/assets/applet/img/jobRecruitment/photo.png);
  background-size: 100% 100%;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_top .userCard .userCard_photo {
  width: 100%;
  height: 100%;
}

.jobRecruitment .jobSearch .jobSearch_top .mine .mine_top .userCard .userCard_name {
