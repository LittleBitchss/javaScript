.noticess{
  position:absolute;
  left: 0;
  bottom: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 200;
}
.noticess .notices{
  position:absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #e5e5e5;
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
  overflow: auto;
}
@keyframes down{
  0%{
    bottom:-70%;
  }
  100%{
    bottom:0%;
  }
}
@keyframes up{
  0%{
    bottom:0%;
  }
  100%{
    bottom:-70%;
  }
}
.noticess .down{
  animation:down 600ms;
}
.noticess .up{
  animation:up 600ms;
}
.noticess .notices .img{
  width: 90%;
  height: 300rpx;
  background-color: #fff;
  margin: 5rpx auto;
  display: block;
}
.noticess .notices .bttn{
  width: 300rpx;
  height: 60rpx;
  font-size: 24rpx;
  margin: 20rpx auto;
  display: block;
  color: #fff;
  background-color: #b6b6b6;
  line-height: 60rpx;
}
.noticess .notices .bttns{
  background-color: #002964 !important;
}

