  left: 50%;
  transform: translateX(-50%);
  bottom: 40rpx;
  background:linear-gradient(to right, #e80e20 ,#ff626f);
}
.familyLocation .process .proces .procesPoints .procesPoint_i{
  height: 14rpx;
  width: 14rpx;
  border-radius: 50%;
  background-color: #fff;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
.familyLocation .process .proces .procesPoint{
  height: 16rpx;
  width: 16rpx;
  border-radius: 50%;
  background:linear-gradient(to right, #e80e20 ,#ff626f);
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
