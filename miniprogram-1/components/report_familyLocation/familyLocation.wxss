  display: flex;
  flex-direction: column;
  position: relative;
}
.familyLocation .process{
  height: 128rpx;
  padding: 0 30rpx;
  position: relative;
  display: flex;
  margin-bottom: 22rpx;
}
.familyLocation .process .processLine{
  position: absolute;
  width: 74%;
  left: 50%;
  bottom: 54rpx;
  transform: translateX(-50%);
  z-index: 0;
  border-top: 2rpx solid #3f6bd6;
}
.familyLocation .process .proces{
  z-index: 1;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
  font-family: 'Light';
  position: relative;
}
.familyLocation .process .proces .procesPoints{
  height: 30rpx;
  width: 30rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  /* align-items: center; */
